background no
cpu_avg_samples 2
net_avg_samples 2
no_buffers yes
out_to_console yes
out_to_x no
total_run_times 0
update_interval 1.0
use_spacer left
pad_percents 3

TEXT
${exec cmus_status} :: \
V${exec amixer get Master | grep "Left" | awk -F'[]%[]' '/%/ { printf "%3d", $2 }'}% :: \
${time %d %b %Y %H:%M}\
;\
D ${downspeed enp0s12} :: \
U ${upspeed enp0s12} :: \
C${cpu cpu1}% :: \
M${memperc}% :: \
S$swapperc% :: \
/${fs_used_perc /}% :: \
/boot${fs_used_perc /boot/}% :: \

